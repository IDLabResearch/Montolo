mon:asymmetricProperties a mov:RestrictionType;
  dc:creator <https://sven-lieber.org/profile#me>;
  dc:description "A property is asymmetric if it is disjoint from its own inverse. A constraint of the constraint type asymmetric object properties states that the object property p is asymmetric - that is, if an individual i_1 is connected by p to an individual i_2, then I_2 cannot be connected by p to i_2.";
  dc:source "https://publikationen.bibliothek.kit.edu/1000054062"^^<http://schema.org/URL>;
  rdfs:isDefinedBy <https://w3id.org/montolo/ns/montolo#>;
  rdfs:label "Asymmetric properties restriction type"@en .

mon:asymmetricPropertiesDetectorOwlAsymmetricProperty a mov:RestrictionTypeExpressionDetector;
  dc:creator <https://sven-lieber.org/profile#me>;
  dc:description "A method or software component to detect owl:AsymmetricProperty restrictions."@en;
  rdfs:isDefinedBy <https://w3id.org/montolo/ns/montolo#>;
  rdfs:label "owl:AsymmetricProperty detector"@en .

mon:asymmetricPropertiesLODStatsDetectorOwlAsymmetricProperty-v1 a mov:RestrictionTypeExpressionDetectorVersion;
  dc:creator <https://sven-lieber.org/profile#me>;
  frbr:realizationOf mon:asymmetricPropertiesDetectorOwlAsymmetricProperty;
  rdfs:isDefinedBy <https://w3id.org/montolo/ns/montolo#>;
  rdfs:label "owl:AsymmetricProperty detector (LODStats) v1"@en .

mon:asymmetricPropertiesOwlAsymmetricProperty a mov:RestrictionTypeExpression;
  dc:creator <https://sven-lieber.org/profile#me>;
  dc:description "An asymmetric properties restriction, expressed using the owl:asymmetricProperty property.";
  frbr:realizationOf mon:asymmetricProperties;
  rdfs:isDefinedBy <https://w3id.org/montolo/ns/montolo#>;
  rdfs:label "OWL asymmetricProperty restriction"@en;
  owl:sameAs <https://astrea.helio.linkeddata.es/owl:AsymmetricProperty> .

